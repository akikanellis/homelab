{{ ansible_managed | comment }}

[core]
  autocrlf = input
  pager = delta

[delta]
  navigate = true
  theme = ansi
  side-by-side = true

[diff]
  colorMoved = default

[interactive]
  diffFilter = delta --color-only

[merge]
  conflictstyle = diff3

